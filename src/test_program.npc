#PROGRAM main;
#using std.map;
#using std.array_list;
#using std.system.println;

int main(string[] argv) {
	if (argv.length < 2) {
		println("ERROR: Program requires at least 1 argument");
		return 2;
	}
	map<int, string> m;
	m[0] = "Hello World";
	for (int i = 0; i < m[0].length; i++) {
		if (m[0].get(i).val() >= 40) {
			println("val greater than 40");
		}
	}
	println("\n");
	println("Program exit successful!");
	return 0;
}
